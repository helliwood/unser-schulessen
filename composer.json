{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "beberlei/doctrineextensions": "^1.2",
    "doctrine/annotations": "^1.13",
    "doctrine/dbal": "^2",
    "doctrine/doctrine-bundle": "^2.5.4",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/migrations": "^3.0",
    "doctrine/orm": "^2.7",
    "doctrine/persistence": "^2.5.2",
    "dompdf/dompdf": "0.8.5",
    "endroid/qr-code-bundle": "^4.1",
    "knplabs/knp-menu-bundle": "^3.0",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpoffice/phpspreadsheet": "^1.9",
    "phpstan/phpdoc-parser": "^0.3.5",
    "ramsey/uuid-doctrine": "^1.5",
    "scienta/doctrine-json-functions": "^4.5",
    "sensio/framework-extra-bundle": "^5.0",
    "symfony/apache-pack": "^1.0",
    "symfony/asset": "^5.0",
    "symfony/console": "^5.0",
    "symfony/dotenv": "^5.0",
    "symfony/expression-language": "^5.0",
    "symfony/flex": "^1.18",
    "symfony/form": "^5.0",
    "symfony/framework-bundle": "^5.0",
    "symfony/http-client": "5.4.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "^5.0",
    "symfony/property-access": "^5.4.0",
    "symfony/property-info": "^5.4.0",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/security-bundle": "^5.0",
    "symfony/sendinblue-mailer": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/translation": "^5.0",
    "symfony/twig-bundle": "^5.0",
    "symfony/validator": "^5.0",
    "symfony/web-link": "^5.0",
    "symfony/webpack-encore-bundle": "^1.3",
    "symfony/yaml": "^5.0",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "phpunit/phpunit": "9.5.26",
    "slevomat/coding-standard": "^5.0",
    "squizlabs/php_codesniffer": "^3.4",
    "symfony/browser-kit": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/debug-bundle": "5.4.*",
    "symfony/maker-bundle": "^1.0",
    "symfony/stopwatch": "5.4.*",
    "symfony/web-profiler-bundle": "5.4.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true,
      "endroid/installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "cs-check": "phpcs -s -n --runtime-set ignore_warnings_on_exit true",
    "cs-fix": "phpcbf",
    "rebuild-testdb": "php bin/console doctrine:schema:drop --full-database --env=test --force && php bin/console doctrine:schema:update --force --env=test -n && php bin/console doctrine:fixtures:load --env=test --append",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*"
    }
  }
}
